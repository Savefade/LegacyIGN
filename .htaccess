RewriteEngine On
RewriteRule ^api/v1/feed/popular/$ ign/popular.php [L]
RewriteRule ^api/v1/feed/timeline/$ ign/timeline.php [L]
RewriteRule ^api/v1/direct_share/inbox/$ ign/messages.php [L]
RewriteRule ^api/v1/accounts/create/$ ign/register.php [L]
RewriteRule ^api/v1/accounts/login/$ ign/login.php [L]
RewriteRule ^api/v1/media/upload/$ ign/upload.php [L]
RewriteRule ^api/v1/activity/recent/$ ign/recent.php [L]
RewriteRule ^api/v1/feed/user/(\d+)/?$ ign/userfeed.php?id=$1 [L]
RewriteRule ^api/v1/friendships/show/(\d+)/?$ ign/friendships.php?id=$1 [L]
RewriteRule ^api/v1/users/(\d+)/info/$ ign/userinfo.php?id=$1 [L]
